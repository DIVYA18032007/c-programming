#include <stdio.h>

int main() {

    float f;

    int sqr, base, power;


    scanf("%f", &f);

    scanf("%d %d %d", &sqr, &base, &power);


    printf("%d\n", (int)floor(f));

    printf("%d\n", (int)ceil(f));

    printf("%d\n", (int)sqrt(sqr));

    printf("%d\n", (int)pow(base, power));

    return 0;

}
